{
    "manifest_version": 2,
    "name": "UpSolve tracker",
    "version": "0.01",
    "browser_action": {
        "default_icon": "images/greytick.png",
        "default_title": "UpSolve tracker",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}